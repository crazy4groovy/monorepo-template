{
  "semi": false,
  "trailingComma": "es5",
  "singleQuote": true,
  "useTabs": false,
  "printWidth": 100,
  "tabWidth": 2,
  "arrowParens": "always",
  "endOfLine": "lf",
  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-svelte"
  ],
  "overrides": [
    {
      "files": "*.astro",
      "options": { "parser": "astro" }
    },
    {
      "files": "*.svelte",
      "options": { "parser": "svelte" }
    }
  ]
}
