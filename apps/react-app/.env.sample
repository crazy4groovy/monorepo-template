# React App Environment Variables
# Copy this file to .env and update with your actual values
# Note: In Vite, only variables prefixed with VITE_ are exposed to the client

# API base URL
VITE_API_URL=http://localhost:3000

# Application name
VITE_APP_NAME=React App

# Environment (development, production, etc.)
VITE_ENV=development

VITE_FIREBASE_API_KEY=#Firebase Console → Project Settings → Your apps
VITE_FIREBASE_AUTH_DOMAIN=#Usually `project-id.firebaseapp.com`
VITE_FIREBASE_PROJECT_ID=#Firebase Console
VITE_FIREBASE_APP_ID=#Firebase Console → Your apps

VITE_WEB3FORMS_ACCESS_KEY=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
