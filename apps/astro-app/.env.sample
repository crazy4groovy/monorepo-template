# Astro App Environment Variables
# Copy this file to .env and update with your actual values
# Note: In Astro, only variables prefixed with PUBLIC_ are exposed to the client

# API base URL
PUBLIC_API_URL=http://localhost:3000

# Application name
PUBLIC_APP_NAME=Astro App

# Environment (development, production, etc.)
# Note: MODE is automatically set by Astro

VITE_WEB3FORMS_ACCESS_KEY=xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
