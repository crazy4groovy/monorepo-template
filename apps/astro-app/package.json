{
  "name": "astro-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint . --ext ts,astro --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,astro --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md,astro,svelte}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md,astro,svelte}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "knip": "knip"
  },
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "astro": "^4.15.0",
    "package1": "workspace:*",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-astro": "^0.34.0",
    "jsdom": "^23.0.1",
    "knip": "^5.70.1",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  }
}
