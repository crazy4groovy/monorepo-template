{
  "name": "astro-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "lint": "eslint . --ext ts,astro --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,astro --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md,astro,svelte}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md,astro,svelte}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "knip": "knip"
  },
  "dependencies": {
    "@astrojs/react": "~4.4.2",
    "astro": "~5.16.0",
    "firebase-auth": "workspace:*",
    "package1": "workspace:*",
    "react": "~19.2.0",
    "react-dom": "~19.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/coverage-v8": "^1.6.1",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-astro": "^1.5.0",
    "jsdom": "^27.2.0",
    "knip": "^5.70.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  }
}
