{
  "name": "svelte-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,svelte --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,svelte --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md,astro,svelte}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md,astro,svelte}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "knip": "knip"
  },
  "dependencies": {
    "package1": "workspace:*",
    "svelte": "~5.43.14"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.9",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^8.57.1",
    "eslint-plugin-svelte": "^2.46.1",
    "jsdom": "^27.2.0",
    "knip": "^5.70.1",
    "svelte-eslint-parser": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^1.6.1"
  }
}
